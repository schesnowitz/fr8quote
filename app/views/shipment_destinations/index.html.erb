<p id="notice"><%= notice %></p>

<h1>Shipment Destinations</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Name</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Contact person</th>
      <th>Notes</th>
      <th>Shipment</th>
      <th>Destination count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipment_destinations.each do |shipment_destination| %>
      <tr>
        <td><%= shipment_destination.date %></td>
        <td><%= shipment_destination.name %></td>
        <td><%= shipment_destination.street %></td>
        <td><%= shipment_destination.city %></td>
        <td><%= shipment_destination.state %></td>
        <td><%= shipment_destination.zip %></td>
        <td><%= shipment_destination.contact_person %></td>
        <td><%= shipment_destination.notes %></td>
        <td><%= shipment_destination.shipment_id %></td>
        <td><%= shipment_destination.destination_count %></td>
        <td><%= link_to 'Show', shipment_destination %></td>
        <td><%= link_to 'Edit', edit_shipment_destination_path(shipment_destination) %></td>
        <td><%= link_to 'Destroy', shipment_destination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipment Destination', new_shipment_destination_path %>
